@{
    ViewData["Title"] = "Prices";
}
@{
    Layout = "~/Views/Home/Layout.cshtml";
}
@section AddHead{
    <link rel="stylesheet" href="~/css/Home/price.css" asp-append-version="true" />
}



<!-- image -->
<img src="~/imgs/group-18.png" class="group-18" />

<!-- center heading -->
<h1 class="d-flex justify-content-center" id="prices-h">Prices</h1>
<div class="d-flex justify-content-center">
    <hr />
    <img src="~/imgs/forma-1-copy-5.png" class="forma-1-copy-5" />
    <hr class="hr2" />
</div>

<!-- one time -->
<div class="container d-flex justify-content-center text-center"
     style="margin-top: 42px">
    <h2 class="onetime">One Time</h2>
</div>

<!-- pricebox start -->
<div class="container d-flex justify-content-center">
    <div style="width: 553px; border: solid 0.5px #1d7a8c; margin-top: -20px">
        <h2 style="
            text-align: center !important;
            font-size: 30px;
            color: #646464;
            line-height: 0.8;
            padding-top: 40px;
          ">
            <i class="fa fa-euro" style="line-height: 0.8; margin-right: 5px"></i>20/hr
        </h2>
        <div class="d-flex" style="margin-top: 30px">
            <p style="
              font-size: 18px;
              color: #646464;
              line-height: 1.5;
              margin-left: 130px;
            ">
                <img src="~/imgs/checkbox.png"
                     style="object-fit: contain; margin-right: 10px" />Lower Prices
            </p>
        </div>
        <div class="d-flex" style="margin-top: 5px">
            <p style="
              font-size: 18px;
              color: #646464;
              line-height: 1.5;
              margin-left: 130px;
            ">
                <img src="~/imgs/checkbox.png"
                     style="object-fit: contain; margin-right: 10px" />Easy online & secure payment
            </p>
        </div>
        <div class="d-flex" style="margin-top: 5px">
            <p style="
              font-size: 18px;
              color: #646464;
              line-height: 1.5;
              margin-left: 130px;
            ">
                <img src="~/imgs/checkbox.png"
                     style="object-fit: contain; margin-right: 10px" />Easy amendment
            </p>
        </div>
    </div>
</div>

<!-- price box complete -->
<!-- line -->

<div class="container d-flex justify-content-center">
    <hr class="line" />
</div>

<!-- line end -->
<!-- extra service start -->
<h2 class="d-flex justify-content-center" id="extra">Extra services</h2>
<div class="d-flex justify-content-center">
    <hr />
    <img src="~/imgs/forma-1-copy-5.png" class="forma-1-copy-5" />
    <hr class="hr2" />
</div>

<div class="container" style="margin-top: 39px">
    <div class="d-flex justify-content-center" style="flex-wrap: wrap">
        <div class="text-center justify-content-center mx-5">
            <div>
                <div class="border border-2 rounded-circle text-center"
                     id="cabinet">
                    <img src="~/imgs/cabinet.png" />
                </div>
            </div>
            <div class="text-center">
                <p class="inside">Inside cabinet<br /><span>30 minutes</span></p>
            </div>
        </div>

        <div class="text-center justify-content-center mx-5">
            <div>
                <div class="border border-2 rounded-circle text-center"
                     style="
                height: 108px;
                width: 108px;
                padding-top: 23px;
                padding-left: 2px;
              ">
                    <img src="~/imgs/fridge.png" />
                </div>
            </div>
            <div class="text-center">
                <p class="inside">Inside fridge<br /><span>30 minutes</span></p>
            </div>
        </div>

        <div class="text-center justify-content-center mx-5">
            <div>
                <div class="border border-2 rounded-circle text-center"
                     style="
                height: 108px;
                width: 108px;
                padding-top: 35px;
                padding-left: 2px;
              ">
                    <img src="~/imgs/oven.png" />
                </div>
            </div>
            <div class="text-center">
                <p class="inside">Inside Oven<br /><span>30 minutes</span></p>
            </div>
        </div>

        <div class="text-center justify-content-center mx-5">
            <div>
                <div class="border border-2 rounded-circle text-center"
                     style="
                height: 108px;
                width: 108px;
                padding-top: 28px;
                padding-left: 2px;
                margin-left: 20px;
              ">
                    <img src="~/imgs/washingmachine.png" />
                </div>
            </div>
            <div class="text-center">
                <p class="inside">
                    Laundry wash & dry<br /><span>30 minutes</span>
                </p>
            </div>
        </div>

        <div class="text-center mx-5">
            <div>
                <div class="border border-2 rounded-circle text-center"
                     style="
                height: 108px;
                width: 108px;
                padding-top: 29px;
                padding-left: 1px;
              ">
                    <img src="~/imgs/window.png" />
                </div>
            </div>
            <div class="text-center">
                <p class="inside">Interior windows<br /><span>30 minutes</span></p>
            </div>
        </div>
    </div>
</div>

<!-- extra service end -->
<!-- cleaning section -->

<div style="margin-top: 61px; background: #f6f6f6f6">
    <div class="d-flex justify-content-center text-center">
        <h2 style="
            margin-top: 80px;
            font-size: 36px;
            font-weight: 500;
            color: #4f4f4f;
            line-height: 0.67;
          ">
            What we include in cleaning
        </h2>
    </div>

    <div class="d-flex justify-content-center">
        <hr style="
            width: 6%;
            color: #4f4f4f;
            margin-top: 16px;
            margin-right: 10px;
            height: 1px;
          " />
        <img src="~/imgs/forma-1-copy-5.png"
             style="margin-top: 0px; object-fit: contain" />
        <hr style="
            width: 6%;
            color: #4f4f4f;
            margin-top: 16px;
            margin-left: 10px;
            height: 1px;
          " />
    </div>

    <div class="container" style="padding-bottom: 80px">
        <div class="row">
            <div class="col-lg-4">
                <img src="~/imgs/badrooms.png"
                     style="margin-top: 42px; width: 100%" />
                <h3 style="
                margin-top: 24px;
                font-size: 20px;
                font-weight: bold;
                color: #4f4f4f;
              ">
                    Bedroom and Living Room
                </h3>
                <p style="color: #4f4f4f; text-align: left">
                    <img src="~/imgs/vector-smart-object-copy.png"
                         style="margin-right: 9px" />Dust all accessible surfaces
                </p>
                <p style="color: #4f4f4f; text-align: left">
                    <img src="~/imgs/vector-smart-object-copy.png"
                         style="margin-right: 9px" />Wipe down all mirrors and glass fixtures
                </p>
                <p style="color: #4f4f4f; text-align: left">
                    <img src="~/imgs/vector-smart-object-copy.png"
                         style="margin-right: 9px" />Clean all floor surfaces
                </p>
                <p style="color: #4f4f4f; text-align: left">
                    <img src="~/imgs/vector-smart-object-copy.png"
                         style="margin-right: 9px" />Take out garbage and recycling
                </p>
            </div>

            <div class="col-lg-4">
                <img src="~/imgs/bathrooms.png"
                     style="margin-top: 42px; width: 100%" />
                <h3 style="
                margin-top: 24px;
                font-size: 20px;
                font-weight: bold;
                color: #4f4f4f;
              ">
                    bathrooms
                </h3>
                <p style="color: #4f4f4f; text-align: left">
                    <img src="~/imgs/vector-smart-object-copy.png"
                         style="margin-right: 9px" />Wash and sanitize the toilet, shower, tub, sink
                </p>
                <p style="color: #4f4f4f; text-align: left">
                    <img src="~/imgs/vector-smart-object-copy.png"
                         style="margin-right: 9px" />Dust all accessible surfaces
                </p>
                <p style="color: #4f4f4f; text-align: left">
                    <img src="~/imgs/vector-smart-object-copy.png"
                         style="margin-right: 9px" />Wipe down all mirrors and glass fixtures
                </p>
                <p style="color: #4f4f4f; text-align: left">
                    <img src="~/imgs/vector-smart-object-copy.png"
                         style="margin-right: 9px" />Clean all floor surfaces
                </p>
                <p style="color: #4f4f4f; text-align: left">
                    <img src="~/imgs/vector-smart-object-copy.png"
                         style="margin-right: 9px" />Take out garbage and recycling
                </p>
            </div>

            <div class="col-lg-4">
                <img src="~/imgs/kitchen.png"
                     style="margin-top: 42px; width: 100%" />
                <h3 style="
                margin-top: 24px;
                font-size: 20px;
                font-weight: bold;
                color: #4f4f4f;
              ">
                    Kitchen
                </h3>
                <p style="color: #4f4f4f; text-align: left">
                    <img src="~/imgs/vector-smart-object-copy.png"
                         style="margin-right: 9px" />Dust all accessible surfaces
                </p>
                <p style="color: #4f4f4f; text-align: left">
                    <img src="~/imgs/vector-smart-object-copy.png"
                         style="margin-right: 9px" />Empty sink and load up dishwasher
                </p>
                <p style="color: #4f4f4f; text-align: left">
                    <img src="~/imgs/vector-smart-object-copy.png"
                         style="margin-right: 9px" />Wipe down all mirrors and glass fixtures
                </p>
                <p style="color: #4f4f4f; text-align: left">
                    <img src="~/imgs/vector-smart-object-copy.png"
                         style="margin-right: 9px" />Clean all floor surfaces
                </p>
                <p style="color: #4f4f4f; text-align: left">
                    <img src="~/imgs/vector-smart-object-copy.png"
                         style="margin-right: 9px" />Take out garbage and recycling
                </p>
            </div>
        </div>
    </div>
</div>

<!-- cleaning section end -->

<div class="container">
    <h2 style="margin-top: 78px; text-align: center">Why Helperland</h2>
</div>
<div class="container d-flex justify-content-center">
    <hr />
    <img src="~/imgs/forma-1-copy-5.png" class="forma-1-copy-5" />
    <hr class="hr2" />
</div>

<!-- helper starts -->
<div class="container" id="helper">
    <div class="row">
        <div class="col-lg-4" style="margin-top: 42px">
            <h3 style="
              color: #646464;
              font-size: 20px;
              font-weight: bold;
              line-height: 1.2;
              text-align: right;
            ">
                Experienced and vetted professionals
            </h3>
            <p style="color: #646464; font-size: 14px; text-align: right">
                dominate the industry in scale and scope with<br />an adaptable,
                extensive network that <br />consistently delivers exceptional
                results.
            </p>
            <h3 style="
              color: #646464;
              font-size: 20px;
              line-height: 1.2;
              font-weight: bold;
              text-align: right;
            ">
                <br />Dedicated customer service
            </h3>
            <p style="color: #646464; font-size: 14px; text-align: right">
                to our customers and are guided in all we do<br />
                by their needs. The team is always happy to <br />support you and
                offer all the information. you need.
            </p>
        </div>
        <div class="col-lg-4" style="margin-top: 42px">
            <img src="~/imgs/cleanservice.png"
                 class="cleanservice"
                 style="width: 100%" />
        </div>
        <div class="col-lg-4" style="margin-top: 42px">
            <h3 style="
              color: #646464;
              font-size: 20px;
              font-weight: bold;
              line-height: 1.2;
            ">
                Every cleaning is insured
            </h3>
            <p style="color: #646464; font-size: 14px">
                and seek to provide exceptional service and<br />
                engage in proactive behavior. We‘d be happy<br />
                to clean your homes.
            </p>

            <h3 style="
              color: #646464;
              font-size: 20px;
              line-height: 1.2;
              font-weight: bold;
            ">
                <br />Secure online payment
            </h3>
            <p style="color: #646464; font-size: 14px">
                Payment is processed securely online.<br />
                Customers pay safely online and manage<br />
                the booking.
            </p>
        </div>
    </div>
</div>

<!-- helper end -->
<button type="button"
        class="btn btn-floating"
        id="btn-back-to-top"
        style="z-index: 100">
    <i class="fa fa-chevron-circle-up"></i>
</button>


@section AddFooter{
    <script src="~/lib/jquery/jquery.js"></script>
    <script src="~/js/Home/price.js" asp-append-version="true"></script>
}