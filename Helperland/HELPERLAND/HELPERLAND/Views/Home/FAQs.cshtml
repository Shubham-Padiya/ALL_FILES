@{
    ViewData["Title"] = "FAQs";
}
@{
    Layout = "~/Views/Home/Layout.cshtml";
}
@section AddHead{
    <link rel="stylesheet" href="~/css/Home/faq.css" asp-append-version="true" />
}
<!-- faq image -->
<img src="~/imgs/group-16.png"
     style="width: 100%; height: 370px; object-fit: cover" />

<!-- center heading -->
<div class="d-flex justify-content-center"
     style="
        color: #4f4f4f;
        font-weight: 500;
        font-size: 36px;
        margin-top: 40px;
      ">
    FAQs
</div>
<div class="d-flex justify-content-center">
    <hr style="
          width: 7%;
          color: #4f4f4f;
          margin-top: 30px;
          margin-right: 10px;
          height: 1px;
        " />
    <img src="~/imgs/forma-1-copy-5.png"
         style="margin-top: 14px; object-fit: contain" />
    <hr style="
          width: 7%;
          color: #4f4f4f;
          margin-top: 30px;
          margin-left: 10px;
          height: 1px;
        " />
</div>
<div class="content">
    <p style="
          text-align: center;
          margin-top: 37px;
          font-size: 18px;
          line-height: 1.44;
          color: #646464;
        ">
        Whether you are Customer or Service provider,<br />
        We have tried our best to solve all your queries and questions.
    </p>
</div>
<!-- finish -->
<!-- large button starts -->
<div class="container mt-5">
    <div class="row justify-content-center">
        <div class="col-4 mt-1 text-center cust"
             style="
            font-size: 24px;
            width: 470px;
            padding-top: 7px;
            padding-bottom: 7px;
            line-height: 2.17;
          ">
            <a href="" data-bs-toggle="collapse" style="text-decoration: none">FOR COUSTOMER</a>
        </div>
        <div class="col-4 mt-1 text-center prov"
             style="
            font-size: 24px;
            width: 470px;
            padding-top: 7px;
            padding-bottom: 7px;
            line-height: 2.17;
          ">
            <a href="" data-bs-toggle="collapse" style="text-decoration: none">FOR SERVICE PROVIDER</a>
        </div>
    </div>
</div>
